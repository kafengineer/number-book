<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/navigation" app:startDestination="@id/contactFragment2">

    <fragment android:id="@+id/contactFragment2"
              android:name="com.example.myownphonecontact.screens.allcontacts.ContactFragment"
              android:label="ContactFragment">
        <action android:id="@+id/action_contactFragment2_to_newContactFragment"
                app:destination="@id/newContactFragment"/>
        <action android:id="@+id/action_contactFragment2_to_contactDetailsFragment"
                app:destination="@id/contactDetailsFragment"/>
    </fragment>
    <fragment android:id="@+id/newContactFragment"
              android:name="com.example.myownphonecontact.screens.newcontact.NewContactFragment"
              android:label="NewContactFragment">
        <action android:id="@+id/action_newContactFragment_to_contactFragment2" app:destination="@id/contactFragment2"/>
    </fragment>
    <fragment android:id="@+id/contactDetailsFragment"
              android:name="com.example.myownphonecontact.screens.contactdetails.ContactDetailsFragment"
              android:label="ContactDetailsFragment">
        <argument android:name="contact" app:argType="com.example.myownphonecontact.database.Contacts"/>
        <action android:id="@+id/action_contactDetailsFragment_to_contactEdit" app:destination="@id/contactEdit"/>
    </fragment>
    <fragment android:id="@+id/contactEdit" android:name="com.example.myownphonecontact.screens.contactedit.ContactEdit"
              android:label="ContactEdit">
        <argument android:name="toBeEditedContact" app:argType="com.example.myownphonecontact.database.Contacts"/>
    </fragment>
</navigation>